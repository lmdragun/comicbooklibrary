<h1>Sign in</h1>

 <%= render 'common/flash' %>
<div class="fill-in">
  <%= form_for :session, url: sessions_path do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>
</div>
<div class="sign-in">
  <%= link_to "forgot password?", new_password_reset_path %>

  <%= f.submit 'Sign in', id: 'signin' %>
</div>
 <% end %>
